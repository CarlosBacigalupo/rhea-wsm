      <phi param="6">70</phi>
      <theta param="7">65</theta>
      <alpha param="8">1</alpha>
      <bl_period param="9">32</bl_period>
    </optical_element>
    <optical_element type="boundary">
      <medium>nkzfs8</medium>
      <phi param="4">90</phi>
      <theta param="5">90</theta>
    </optical_element>
    <optical_element type="boundary">
      <medium>air</medium>
      <phi param="2">60</phi>
      <theta param="3">90</theta>
    </optical_element>
  </optical_elements>
